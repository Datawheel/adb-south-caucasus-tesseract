<Schema name="adb-south-caucasus">

    <!--Trade Values-->
    <Cube name="Trade Values">
    
        <Table name="trade_values"/>
            
            <!--Dimension-->    
            <Dimension foreign_key="year" name="Year">
                <Hierarchy name="Year" primary_key="year">
                    <Level name="Year" key_column="year" />
                </Hierarchy>
            </Dimension>
        
<!--


           <DimensionUsage name="Importer" source="Geography" foreign_key="importer_country_id" type="geo" />


            <Dimension foreign_key="dim_year" name="Year">
            <Hierarchy name="Year" primary_key="dim_year">
                <Level name="Year" key_column="dim_year" />
                <Property name="Collection Year(s)" column="dim_collection_year"/>
                </Level>
            </Hierarchy>
            </Dimension>
            <Dimension foreign_key="exporter_country_id" name="Exporter Country">
                <Hierarchy name="Exporter Country" primary_key="exporter_country_id">
                    <Level name="Exporter Country" key_column="exporter_country_id"/>

                </Hierarchy>
            </Dimension>  



            <Dimension foreign_key="importer_country_id" name="Importer Country">
                <Hierarchy name="Importer Country" primary_key="importer_country_id">
                    <Level name="Importer Country" key_column="importer_country_id"/>
                </Hierarchy>
                            <Property name="ISO 3" column="iso_3"/>
            </Dimension>  
-->

            <Dimension foreign_key="importer_country_id" name="Importer Country">
                <Hierarchy name="Geography">
                    <Table name="dim_countries"/>
                        <Level name="Importer Country" key_column="oec_id" name_column="comtrade_name" key_type="text">
                        <Property name="Importer Country ISO 3" column="iso_3"/>
                        </Level>
                </Hierarchy>
            </Dimension>  

            <Dimension foreign_key="exporter_country_id" name="Exporter Country">
                <Hierarchy name="Geography">
                    <Table name="dim_countries"/>
                        <Level name="Exporter Country" key_column="oec_id" name_column="comtrade_name" key_type="text">
                        <Property name="Exporter Country ISO 3" column="iso_3"/>
                        </Level>
                </Hierarchy>
            </Dimension>  

            <!--Measure-->
            <Measure name="Trade Values" column="trade_value" aggregator="sum"/>
    </Cube>

    <!--Gini-->
    <Cube name="Gini">
    
        <Table name="gini"/>
            
            <!--Dimension-->    
            <Dimension foreign_key="year" name="Year">
                <Hierarchy name="Year" primary_key="year">
                    <Level name="Year" key_column="year" />
                </Hierarchy>
            </Dimension>

            <Dimension foreign_key="country_id" name="Country">
                <Hierarchy name="Geography">
                    <Table name="dim_countries"/>
                        <Level name="Country" key_column="iso_3" name_column="comtrade_name" key_type="text">
                        <Property name="Exporter Country OEC ID" column="oec_id"/>
                        </Level>
                </Hierarchy>
            </Dimension>  

            <!--Measure-->
            <Measure name="Gini Index" column="gini" aggregator="avg"/>
    </Cube>


    <!--GDP-->
    <Cube name="GDP">
    
        <Table name="gdp"/>
            
            <!--Dimension-->    
            <Dimension foreign_key="year" name="Year">
                <Hierarchy name="Year" primary_key="year">
                    <Level name="Year" key_column="year" />
                </Hierarchy>
            </Dimension>

            <Dimension foreign_key="country_id" name="Country">
                <Hierarchy name="Geography">
                    <Table name="dim_countries"/>
                        <Level name="Country" key_column="iso_3" name_column="comtrade_name" key_type="text">
                        <Property name="Exporter Country OEC ID" column="oec_id"/>
                        </Level>
                </Hierarchy>
            </Dimension>  
            <!--Measure-->
            <Measure name="Gross Domestic Product (GDP)" column="gdp" aggregator="sum"/>
    </Cube>

    <!--Emissions-->
    <Cube name="Emissions ECI">
        <Table name="emissions"/>

            <Dimension foreign_key='year' name='year'>
                <Hierarchy name='Year' primary_key='year'>
                    <Level name='Year' key_column='year'/>
                </Hierarchy>
            </Dimension>

            <Dimension foreign_key="country" name="Country">
                <Hierarchy name="Geography">
                    <Table name="dim_countries"/>
                        <Level name="Country" key_column="iso_3" name_column="comtrade_name" key_type="text">
                        <Property name="Exporter Country OEC ID" column="oec_id"/>
                        </Level>
                </Hierarchy>
            </Dimension>  
            <!--Measure-->
            <Measure name="Population" column="population" aggregator="sum"/>
            <Measure name="Exports" column="exports" aggregator="sum"/>
            <Measure name="ECI Trade" column="eci_trade" aggregator="sum"/>
            <Measure name="Fitness Trade" column="fitness_trade" aggregator="sum"/>
            <Measure name="ECI Publications" column="eci_publications" aggregator="sum"/>
            <Measure name="Fitness Publications" column="fitness_publications" aggregator="sum"/>
            <Measure name="Emissions" column="documents" aggregator="sum"/>
            <Measure name="Emissions" column="citations" aggregator="sum"/>
            <Measure name="Emissions" column="eci_patents" aggregator="sum"/>
            <Measure name="Fitness Patents" column="fitness_patents" aggregator="sum"/>
            <Measure name="Patents" column="patents" aggregator="sum"/>
            <Measure name="ECI Services" column="eci_services" aggregator="sum"/>
            <Measure name="Fitness Services" column="fitness_services" aggregator="sum"/>
            <Measure name="Service Exports" column="service_exports" aggregator="sum"/>
            <Measure name="GDP Start" column="gdp_start" aggregator="sum"/>
            <Measure name="Emissions" column="emissions" aggregator="sum"/>
            <Measure name="Hum Cap" column="hum_cap" aggregator="sum"/>
            <Measure name="ECI Similarity Trade" column="eci_similarity_trade" aggregator="sum"/>
            <Measure name="ECI Similarity Publications" column="eci_similarity_publications" aggregator="sum"/>
            <Measure name="ECI Similarity Patents" column="eci_similarity_patents" aggregator="sum"/>
            <Measure name="ECI Similarity Services" column="eci_similarity_services" aggregator="sum"/>
            <Measure name="Entropy Trade" column="entropy_trade" aggregator="sum"/>
            <Measure name="Entropy Publications" column="entropy_publications" aggregator="sum"/>
            <Measure name="Entropy Patents" column="entropy_patents" aggregator="sum"/>
            <Measure name="Entropy Services" column="entropy_services" aggregator="sum"/>
            <Measure name="HHI Trade" column="hhi_trade" aggregator="sum"/>
            <Measure name="HHI Publications" column="hhi_publications" aggregator="sum"/>
            <Measure name="HHI Patents" column="hhi_patents" aggregator="sum"/>
            <Measure name="HHI Services" column="hhi_services" aggregator="sum"/>
            <Measure name="Nat Res" column="nat_res" aggregator="sum"/>
    </Cube>



</Schema>